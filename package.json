{
	"devDependencies": {
		"@vue/test-utils": "^2.0.0-rc.18",
		"vue-jest": "^5.0.0-alpha.10"
	},
	"scripts": {
		"test": "jest",
		"dev": "your-dev-command"
	},
	"uni-app": {
		"scripts": {
			"h5-production": {
				"title": "H5生产环境",
				"browser": "chrome",
				"env": {
					"UNI_PLATFORM": "h5",
					"BASE_URL": "https://cat-fact.herokuapp.com"
				},
				"define": {
					"PRODUCTION": true
				}
			},
			"h5-testing": {
				"title": "H5测试环境",
				"browser": "chrome",
				"env": {
					"UNI_PLATFORM": "h5",
					"BASE_URL": "https://cat-fact.herokuapp.com"
				},
				"define": {
					"TESTING": true
				}
			},
			"h5-dev": {
				"title": "H5开发环境",
				"browser": "chrome",
				"env": {
					"UNI_PLATFORM": "h5",
					"BASE_URL": "https://cat-fact.herokuapp.com"
				},
				"define": {
					"DEV": true
				}
			}
		}
	},
	"contributes": {
		"formator": {
			"name": "Prettier",
			"class": "prettier_service.js",
			"filetypes": [
				"less",
				"sass",
				"scss",
				"vue",
				"ux",
				"ts",
				"foo"
			]
		}
	}

}